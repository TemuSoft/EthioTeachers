<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#F0F8FF"
    android:gravity="center"
    tools:context="ethioteacher.dejen.ethioteachers.CreateAccount">

    <TextView
        android:id="@+id/textView34"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/textView57"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentTop="true"
        android:layout_alignStart="@+id/textView57"
        android:gravity="center"
        android:textColor="#009900"
        tools:ignore="HardcodedText,RtlHardcoded" />

    <TextView
        android:id="@+id/textView35"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/textView34"
        android:layout_marginTop="57dp"
        android:text="User name"
        tools:ignore="HardcodedText,RtlHardcoded" />

    <TextView
        android:id="@+id/textView47"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/editText39"
        android:layout_alignBottom="@+id/editText39"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:text="Password"
        tools:ignore="HardcodedText,RtlHardcoded" />

    <TextView
        android:id="@+id/textView48"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/editText44"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:text="Confirm password"
        tools:ignore="HardcodedText,RtlHardcoded" />

    <Button
        android:id="@+id/button5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_below="@+id/editText44"
        android:layout_marginTop="106dp"
        android:background="@android:color/transparent"
        android:text="Sign In"
        tools:ignore="HardcodedText,RtlHardcoded" />

    <Button
        android:id="@+id/button6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/button5"
        android:layout_alignBottom="@+id/button5"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:background="@android:color/transparent"
        android:text="reset"
        tools:ignore="HardcodedText,RtlHardcoded" />

    <EditText
        android:id="@+id/editText20"
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_alignTop="@+id/textView35"
        android:ems="10"
        android:inputType="textPersonName"
        android:maxLength="25"
        android:textSize="13sp"
        tools:ignore="HardcodedText,LabelFor,RtlHardcoded" />

    <EditText
        android:id="@+id/editText39"
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/editText20"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_alignStart="@+id/editText20"
        android:layout_below="@+id/editText20"
        android:ems="10"
        android:inputType="textPassword"
        android:maxLength="12"
        android:textSize="13sp"
        tools:ignore="LabelFor,RtlHardcoded" />

    <EditText
        android:id="@+id/editText44"
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/editText39"
        android:layout_alignStart="@+id/editText39"
        android:layout_below="@+id/editText39"
        android:ems="10"
        android:inputType="textPassword"
        android:maxLength="12"
        android:textSize="13sp"
        tools:ignore="LabelFor,RtlHardcoded" />

    <TextView
        android:id="@+id/textView57"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/button6"
        android:layout_alignParentEnd="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentStart="true"
        android:layout_marginBottom="36dp"
        android:gravity="center"
        android:textColor="#ff0000"
        tools:ignore="RtlHardcoded" />

</RelativeLayout>
